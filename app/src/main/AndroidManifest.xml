<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.materialtheme">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SuperMaterialTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".Buttons" />
        <activity android:name=".ToolBar" />
        <activity android:name=".NavigationDrawer" />
        <activity android:name=".SelectionControls" />
        <activity android:name=".Dialogs" />
        <activity android:name=".SnackbarToast" />
        <activity android:name=".ProgressBar" />
        <activity android:name=".Card" />
        <activity
            android:name=".CollapsingLayout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".RecyclerView" />
        <activity android:name=".BottomNavigation" />
        <activity android:name=".ConstraintLayout"></activity>
        <activity android:name=".AnimationsTransitions"
            android:theme="@style/AppTheme.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailActivity"></activity>
    </application>

</manifest>